</quotepara><quotepara depth="1">sub EssaiLoad

</quotepara><quotepara depth="1">        dim args()
        dim oDesktop as object, oDocument as object
        dim surl as string
        oDesktop=createUnoService("com.sun.star.frame.Desktop")
        sURL=ConvertToURL("c:\essai.doc")
        oDocument=oDesktop.loadComponentFromURL(surl,"_blank",0,args())

</quotepara><quotepara depth="1">end sub

